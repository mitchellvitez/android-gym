<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Gym" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/primary_normal</item>
        <item name="colorOnPrimary">@color/primary_extreme</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/secondary_normal</item>
        <item name="colorOnSecondary">@color/secondary_extreme</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">@color/background_distinguished</item>
        <!-- Customize your theme here. -->
        <item name="android:background">@color/background</item>
        <item name="android:fontFamily">@font/andada_regular</item>
        <item name="fontFamily">@font/andada_regular</item>
    </style>
</resources>